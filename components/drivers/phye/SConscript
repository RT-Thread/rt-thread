from building import *

group = []

if not GetDepend(['RT_USING_PHYE']):
    Return('group')

cwd = GetCurrentDir()
CPPPATH = [cwd + '/../include']

src = ['phye.c']

if GetDepend(['RT_PHYE_GENERIC_USB']):
    src += ['phye-generic-usb.c']

group = DefineGroup('DeviceDrivers', src, depend = [''], CPPPATH = CPPPATH)

Return('group')
