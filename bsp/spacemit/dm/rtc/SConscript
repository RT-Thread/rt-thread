from building import *

group = []
src = []
cwd = GetCurrentDir()
CPPPATH = [cwd + '/../include', cwd + '/../../../../components/drivers/rtc']

if GetDepend(['RT_RTC_SPACEMIT_PMIC']):
    src += ['rtc-spacemit-pmic.c']

group = DefineGroup('DeviceDrivers', src, depend = [''], CPPPATH = CPPPATH)

Return('group')
