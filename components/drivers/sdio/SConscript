Import('RTT_ROOT')
from building import *

cwd     = GetCurrentDir()

src = Split("""
block_dev.c
mmcsd_core.c
sd.c
""")

# The set of source files associated with this SConscript file.
path = [cwd]

group = DefineGroup('Drivers', src, depend = ['RT_USING_MMCSD'], CPPPATH = path)

Return('group')
